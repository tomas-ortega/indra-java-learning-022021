package com.indra.c3po.tutorial.domain;

public class PersonDTO {
	private Integer id;
	private String name;
	
	public PersonDTO() {
		this.id = null;
		this.name = null;
	}
	
	public Integer getId() {
		return id;
	}
	
	public void setId(Integer id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}

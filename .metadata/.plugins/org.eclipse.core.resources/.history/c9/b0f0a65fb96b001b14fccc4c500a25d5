package com.indra.c3po.tutorial.domain;

public class PersonDTO {
	private Integer dni;
	private String name;
	
	public PersonDTO() {
		this.dni = null;
		this.name = null;
	}
	
	public Integer getId() {
		return dni;
	}
	
	public void setId(Integer id) {
		this.dni = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}

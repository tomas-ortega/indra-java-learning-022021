package com.indra.c3po.tutorial.domain;

public class PatientDTO extends PersonDTO {
	private String nhc;

	
	@Override
	public String getName() {
		return this.name.toUpperCase();
	}
	
	@Override
	public Integer getDni() {
		return super.getDni();
	}
	
	@Override
	public static Boolean checkDni(Integer number) {
		return true;
	}
}
